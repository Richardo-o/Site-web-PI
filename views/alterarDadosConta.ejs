<div class="formulario">
    <h1>Alterar Dados</h1>
    <form class="row g-3">
        <div class="col-md-6">
          <label for="inputEmail4" class="form-label">Email</label>
          <input type="email" class="form-control" id="inputEmail4" placeholder="Novo E-mail">
        </div>
        <div class="col-md-6">
          <label for="inputPassword4" class="form-label">Password</label>
          <input type="password" class="form-control" id="inputPassword4" placeholder="Nova Senha">
        </div>
        <div class="col-12">
          <label for="inputAddress" class="form-label">Email Atual</label>
          <input type="text" class="form-control" id="inputAddress" placeholder="Digite o email atual">
        </div>
        <div class="col-12">
          <label for="inputAddress2" class="form-label">Senha atual</label>
          <input type="text" class="form-control" id="inputAddress2" placeholder="Digite a senha atual">
        </div>
        <div class="col-12">
            <button type="submit" class="btn btn-primary">Salvar Dados</button>
          </div>
        </div>
       
      </form>
</div>

